<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - 我的 ASP.NET 应用程序</title>
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")

</head>
<body>
	<div class="container body-content">
		<div class="row">
			<div class="col-md-9">
				@RenderBody()
			</div>
			<div class="col-md-3">
				<ul>
					<li><a href="~/sql/Asterisk">Asterisk query</a></li>
					<li><a href="~/sql/ColumnAlias">Column Alias</a></li>
					<li><a href="~/sql/Multitable">Multi-table query</a></li>
					<li><a href="~/sql/SelfRelationship">Self Relationship query</a></li>
					<li><a href="~/sql/Subquery">Subquery</a></li>
					<li><a href="~/sql/Paging">Paging Query</a></li>
					<li><a href="~/sql/Aggregation">Aggregation and Group By</a></li>
					<li><a href="~/sql/DateGroup">Aggregation with Date</a></li>
				</ul>
			</div>
		</div>
		<hr />
		<footer>
			<p>&copy; @DateTime.Now.Year - APQuery.net WebApplication Demo</p>
		</footer>
	</div>
	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@Scripts.Render("~/bundles/jqery-template")
	@RenderSection("scripts", required: false)
</body>
</html>
